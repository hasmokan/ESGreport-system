{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "vite-env.d.ts.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "vue.d.ts"
  ],
  "compilerOptions": {
    "types": ["element-plus/global"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "preserveValueImports": false
  },

  "references": [{
    "path": "./tsconfig.node.json"
  }]
}